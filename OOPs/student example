class student:
    def __init__ (self,name,roll_num,marks):
        self.name =name
        self.roll_num =roll_num
        self.marks = marks
    def display (self):
        print(self.name,self.roll_num,self.marks)
    def result(self):
        if self.marks>=70:
            print(self.marks)
        else:
            print("marks<70")
obj1=student("vijaya",205,80)
obj1.display()
obj1.result()


output:
vijaya 205 80
80
